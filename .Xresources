XTerm*termname: xterm-color
XTerm*faceName: Consolas
XTerm*faceSize: 12
XTerm*loginShell: true
XTerm*multiscroll: true	
XTerm*foreground: gray90
XTerm*background: gray12
XTerm*cursorBlink: True
XTerm*cursorOffTime: 500
XTerm*cursorOnTime: 500
XTerm*cursorColor: #6ccc6c
XTerm*borderColor: white
XTerm*scrollColor: black
XTerm*highlightColor: #386466
XTerm*highlightSelection: true
XTerm*visualBell: false
XTerm*saveLines: 1000
XTerm*metaSendsEscape: true
XTerm*internalBorder:  4

Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

Xcursor.theme: Oxygen_Oxygen

URxvt*font: xft:Consolas-12:antialias=true
URxvt*scollbar: false
URxvt*scrollBar: False
URxvt*buffered: true
URxvt*cursorColor: green
URxvt*saveLines: 8192

URxvt.perl-ext-common: default,matcher,clipboard
URxvt.urlLauncher: /usr/bin/chromium
URxvt.matcher.button: 1

!!! Terminal Colors

*foreground: gray90
*background: gray12

! Black + DarkGrey
*color0: black
*color8: #586363

! DarkRed + Red
*color1: orange red
*color9: salmon3

! DarkGreen + Green
*color2: #6ccc6c
*color10: green

! DarkYellow + Yellow
*color3: goldenrod
*color11: goldenrod

! DarkBlue + Blue
*color4: skyblue3
*color12: skyblue3

! DarkMangenta + Mangenta
*color5: magenta3
*color13: magenta

!DarkCyan + Cyan (both not tango)
*color6: cyan3
*color14: cyan

! LightGrey + White
*color7: gray90
*color15: #cecece
