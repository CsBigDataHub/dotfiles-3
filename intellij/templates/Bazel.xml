<templateSet group="Bazel">
  <template name="vispub" value="visibility = [&quot;//visibility:public&quot;]," description="Bazel visibility private" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
      <option name="Python" value="false" />
    </context>
  </template>
  <template name="visppub" value="package(default_visibility = [&quot;//visibility:public&quot;])&#10;" description="Bazel package visibility public" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
